{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".api.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@ab/api/(.*)$": "<rootDir>/../src/api/$1",

    "^@ab/(.*)$": "<rootDir>/../src/$1"
  }
}
